.board
  width 100%
  height 100%
  min-height 100px
  max-width 1920px
  min-width 620px

.board-cell
  width 33%
  height 33%
  display inline-block
  padding 5px
  margin-left -1px
  margin-bottom -1px

  &_0
    border 1px solid red

  &_1
    border 1px solid green

  &_2
    border 1px solid blue

.board-row
  width 100%
